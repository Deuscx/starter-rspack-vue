  "dependencies": {
    "@vueuse/core": "^13.7.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@iconify-json/carbon": "^1.2.13",
    "@rsbuild/core": "^1.4.15",
    "@rsbuild/plugin-vue": "^1.1.1",
    "@types/node": "^22.17.2",
    "@unocss/eslint-config": "^66.4.2",
    "@unocss/eslint-plugin": "^66.4.2",
    "@unocss/webpack": "^66.4.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "pathe": "^2.0.3",
    "rspack-plugin-virtual-module": "^1.0.1",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.1.0",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "resolutions": {
    "unplugin": "^2.3.6",
    "vite": "^7.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
